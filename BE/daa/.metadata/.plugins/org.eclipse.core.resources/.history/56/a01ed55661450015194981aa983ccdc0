    Scanner in = new Scanner(System.in);
    Vector threadPool = new Vector();
    
    System.out.println("\nEnter num1: ");
    num1 = in.nextLong();
    System.out.println("\nEnter num2: ");
    num2 = in.nextLong();
    
    int i = 1;
    
    while (num2 = num2%10) {
      
    }
//  Init i = 1
//    take a digit from num2 unless it becomes zero
//    send i, the digit and num1 to a thread
//    run thread
//    add the thread to a Collection
    in.close();
